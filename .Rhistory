merged_data <- merge(merged_data, minors, by = c("Student.ID", "Term.Code"), all.x = TRUE, all.y = TRUE)
merged_data = merged_data %>% select(-`Cohort.Term.y`)
thematic <- read_excel("BA Major Students - Thematic Sequence ISA2.xlsx")
